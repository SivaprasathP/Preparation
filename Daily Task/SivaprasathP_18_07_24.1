package zoho;

import java.util.*;

public class Demo {
	
	public static void main(String[] args) {
		add(3,4);
		add(354l,6578l);
		add((byte)4,(byte)5);
		add((short)10,(short)67);
		add(5.90f,7.8f);
		add(6.777,9.4546);
		
		sub(3,4);
