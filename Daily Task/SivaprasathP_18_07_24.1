		short sub=(short)(a-b);
		System.out.println("Subtraction :"+sub);
	}
	
	public static void sub(byte a,byte b) {
		byte sub=(byte)(a-b);
		System.out.println("Subtraction :"+sub);
	}
	
	public static void sub(float a,float b) {
		float sub=a-b;
		System.out.println("Subtraction :"+sub);
	}
	
	public static void sub(double a,double b) {
		double sub=a-b;
		System.out.println("Subtraction :"+sub);
	}
	
	public static void sub(long a,long b) {
		long sub=a-b;
		System.out.println("Subtraction :"+sub);
	}
	
	public static void mul(int a,int b) {
		int m=a*b;
		System.out.println("Multiplication :"+m);
	}
	
	public static void mul(byte a,byte b) {
		int m=a*b;
		System.out.println("Multiplication :"+m);
	}
	
	public static void mul(short a,short b) {
		int m=a*b;
		System.out.println("Multiplication :"+m);
	}
	
	public static void mul(long a,long b) {
		long m=a*b;
		System.out.println("Multiplication :"+m);
	}
	
	public static void mul(double a,double b) {
		double m=a*b;
		System.out.println("Multiplication :"+m);
	}
	
	public static void mul(float a,float b) {
		double m=a*b;
		System.out.println("Multiplication :"+m);
