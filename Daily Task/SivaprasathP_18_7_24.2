package zoho;

public class Movie {
	String name;
	
	Movie(String name){
		this.name=name;
	}
	
	void display() {
		System.out.println(this.name);
	}
	
	public static void main(String[] args) {
		Movie r=new RomComMovie("500 days of summer","Romance");
		r.display();
		
		Movie t=new ThrillerMovie("Old Boy","Thriller");
		t.display();
		
		Movie c=new ComedyMovie("HangOver");
		c.display();
	}

}
class RomComMovie extends Movie{
	String type="RomCom";
	
	RomComMovie(String name,String type) {
		super(name);
		this.type=type;
	}

	void display() {
		System.out.println(this.name + " "+this.type);
	}
	
}
class ThrillerMovie extends Movie{
	String type="Thriller";
	
	ThrillerMovie(String name,String type) {
		super(name);
		this.type=type;
	}
	
	void display() {
		System.out.println(this.name + " "+this.type);
	}

}
class ComedyMovie extends Movie{
	String type="Comedy";
	
	ComedyMovie(String name){
		super(name);
	}
	
	void display() {
		System.out.println(this.name + " "+this.type);
	}
}
