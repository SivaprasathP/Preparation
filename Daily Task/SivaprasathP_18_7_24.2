package zoho;

public class Employee {
	int id;
	String name;
	String role;
	
	Employee(int id,String name,String role) {
		this.id=id;
		this.name=name;
		this.role=role;
	}
	
	void getEmployeeDetails() {
		System.out.println(id +" "+name+" "+role);
	}
	
	public static void main(String[] args) {
		Employee h=new HourlyEmployee(1,"Ram","HR");
		h.getEmployeeDetails();
		
		Employee s=new SalariedEmployee(2,"Raghu","Technical");
		s.getEmployeeDetails();
	}

}

class HourlyEmployee extends Employee{
	String type="Hourly";
	
	HourlyEmployee(int id,String name,String role){
		super(id,name,role);
	}
	
	void getEmployeeDetails() {
		System.out.println(id +" "+name+" "+role+"Hourly Employee");
	}
	
}

class SalariedEmployee extends Employee{
    String type="Salaried";
	
	SalariedEmployee(int id,String name,String role){
		super(id,name,role);
	}
	
	void getEmployeeDetails() {
		System.out.println(id +" "+name+" "+role+"Salaried Employee");
	}
}





